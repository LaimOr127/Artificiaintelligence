     d2 = 1\n","        else:\n","            n2 = o.num\n","            d2 = o.den\n","        n = n1 * d2 + n2 * d1\n","        d = d1 * d2\n","        return Rational(n, d)\n","    \n","    def __radd__(self, o):\n","        n1 = self.num\n","        d1 = self.num\n","        if type(o) == int:\n","            n2 = o\n","            d2 = 1\n","        else:\n","            n2 = o.num\n","            d2 = o.den\n","        n = n1 * d2 + n2 * d1\n","        d = d1 * d2\n","        return Rational(n, d)\n","    \n","    def __sub__(self, o):\n","        n1 = self.num\n","        d1 = self.den\n","        n2 = o.num\n","        d2 = o.den\n","        n = n1 * d2 - n2 * d1\n","        d = d1 * d2\n","        return Rational(n, d)\n","    \n","    def __mul__(self, o):\n","        n1 = self.num\n","        d1 = self.den\n","        n2 = o.num\n","        d2 = o.den\n","        n = n1 * n2\n","        d = d1 * d2\n","        return Rational(n, d)\n","    \n","    def __floordiv__(self, o):\n","        n1 = self.num\n","        d1 = self.den\n","        n2 = o.num\n","        d2 = o.den\n","        n = n1 * d2\n","        d = d1 * n2\n","        return Rational(n, d)\n","    \n","\n","d1 = Rational(1, 2)\n","d2 = Rational(1, 3)\n","d3 = d1 + d2\n","print(d3)\n","d4 = d1 - d2\n","print(d4)\n","d5 = d1 * d2\n","print(d5)\n","d6 = d1 * d2\n","print(d6)\n","d7 = d1 // d2\n","print(d7)\n","d8 = 6 + d1\n","print(d8)"]},{"cell_type":"markdown","metadata":{"id":"mOhNQnoZAT0B"},"source":["Задание\n","Создайте класс по работе с тригонометрическими функциями. В классе должны быть реализованы функции вычисления:\n","\n","косинуса;\n","синуса;\n","тангенса;\n","арксинуса;\n","арккосинуса;\n","арктангенса;\n","перевода из градусов в радианы."]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"background_save":true},"id":"TEMin0vyAUDn"},"outputs":[{"name":"stdout","output_type":"stream","text":["Cos(3.0): -0.9899924966004454\n","Sin(3.0): 0.1411200080598672\n","Tg(3.0): -0.1425465430742778\n","Arcsin(3.0): error: input value out of range\n","Arccos(3.0): error: input value out of range\n","Arctg(3.0): 1.2490457723982544\n","45 градусов в радианах: 0.05235987755982989\n"]}],"source":["import math \n","\n","class Trigonometry: \n","    def __init__(self, arg): \n","        self.arg = arg \n","    \n","    def cos(self): \n","        return math.cos(self.arg) \n","    \n","    def sin(self): \n","        return math.sin(self.arg) \n","    \n","    def tg(self): \n","        return math.tan(self.arg) \n","    \n","    def arcsin(self): \n","        if self.arg \u003c -1 or self.arg \u003e 1: \n","            return \"error: input value out of range\" \n","        return math.asin(self.arg) \n","    \n","    def arccos(self): \n","        if self.arg \u003c -1 or self.arg \u003e 1: \n","            return \"error: input value out of range\" \n","        return math.acos(self.arg) \n","    \n","    def arctg(self): \n","        if self.arg == -1: \n","            return -math.pi / 2 \n","        elif self.arg == 1: \n","            return math.pi / 2 \n","        else: \n","            return math.atan(self.arg) \n","    \n","    def from_degrees_to_radians(self): \n","        self.arg *= math.pi / 180 \n","        return self.arg \n","\n","# Пример использования класса \n","arg = float(input(\"Введите аргумент в зайчиках: \"))\n","t = Trigonometry(arg)  # создаем экземпляр класса, передаем аргумент в радианах \n","\n","print(f\"Cos({t.arg}):\", t.cos())  # выводим результат вычисления косинуса \n","print(f\"Sin({t.arg}):\", t.sin())  # выводим результат вычисления синуса \n","print(f\"Tg({t.arg}):\", t.tg())  # выводим результат вычисления тангенса \n","print(f\"Arcsin({t.arg}):\", t.arcsin())  # выводим результат вычисления арксинуса \n","print(f\"Arccos({t.arg}):\", t.arccos())  # выводим результат вычисления арккосинуса \n","print(f\"Arctg({t.arg}):\", t.arctg())  # выводим результат вычисления арктангенса \n","t.from_degrees_to_radians() \n","print(f\"45 градусов в радианах:\", t.arg)  # выводим результат перевода 45 градусов в радианы\n"]},{"cell_type":"markdown","metadata":{"id":"00gYeRqBEEyN"},"source":["1.2.1 Пример\n","Определите класс бинарного дерева и задайте его объекты с отдельным атрибутом для каждого из потомков."]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":36},"executionInfo":{"elapsed":999,"status":"ok","timestamp":1682423726187,"user":{"displayName":"BPS-BonsPiSe","userId":"16948838022840116531"},"user_tz":-180},"id":"x8882jSREFHF","outputId":"62530dab-6a52-4ff6-8458-fe04feac578d"},"outputs":[{"data":{"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"},"text/plain":["'c'"]},"execution_count":6,"metadata":{},"output_type":"execute_result"}],"source":["class Tree:\n","    def __init__(self, left, right):\n","        self.left = left\n","        self.right = right\n","        \n","t = Tree(Tree('a', 'b'), Tree('c', 'd'))\n","t.right.left"]},{"cell_type":"markdown","metadata":{"id":"KWxifMkIEPKa"},"source":["1.2.2 Пример\n","Для обозначения отсутствующих потомков можно использовать None (в случае если у узла только один потомок). Само собой, можно комбинировать разные методы (например, использовать списки или множества потомков для каждого узла)."]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":36},"executionInfo":{"elapsed":42,"status":"ok","timestamp":1682423364786,"user":{"displayName":"BPS-BonsPiSe","userId":"16948838022840116531"},"user_tz":-180},"id":"W1lGM4iDEPjL","outputId":"b8d7c86e-1737-4813-b7a3-45da6a540f20"},"outputs":[{"data":{"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"},"text/plain":["'c'"]},"execution_count":3,"metadata":{},"output_type":"execute_result"}],"source":["class Tree:\n","    def __init__(self, kids, next=None):\n","        self.kids = self.val = kids\n","        self.next = next\n